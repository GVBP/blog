<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Page Title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
</head>

<body>
    <tbody>
        <% if(!docs || docs.length == 0) { %>
            <tr>
                <td>Nenhum cadastro.</td>
            </tr>
            <% } else {
        docs.forEach(function(item){ %>
                <tr>
                    <td>
                            <%= item._id %>
                    </td>
                    <td>
                        <%= item.nome %>
                    </td>
                    <td>
                        <%= item.email %>
                    </td>
                </tr>
                <% })
        }%>
    </tbody>
</body>

</html>